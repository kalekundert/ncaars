test_config_path:
  -
    scaffold: s = Scaffold('.')
    expected: CWD / 'config.nt'
  -
    scaffold: s = Scaffold('mma_pylrs')
    expected: BUILTIN_SCAFFOLDS / 'mma_pylrs' / 'config.nt'

test_pdb_path:
  -
    scaffold: s = Scaffold('.')
    expected: CWD / 'scaffold.pdb'
  -
    scaffold:
      > s = Scaffold('.')
      > s.pdb_relpath = 'input.pdb'
    expected: CWD / 'input.pdb'
  -
    scaffold: s = Scaffold('mma_pylrs')
    expected: BUILTIN_SCAFFOLDS / 'mma_pylrs' / '2zim_relaxed.pdb'
  -
    scaffold:
      > s = Scaffold('.')
      > s.pdb_relpath = 'input.cif'
    error:
      type: ConfigError
      pattern:
        - PDB path must have '.pdb' extension
        - config: .*config\.nt
        - PDB path: .*input\.cif

test_scorefxn:
  -
    files:
      config.nt:
        > scaffold:
        >   scorefxn: xyz
    expected: xyz
  -
    scaffold: s = Scaffold('mma_pylrs')
    expected: ref2015

